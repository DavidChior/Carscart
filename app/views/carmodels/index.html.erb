<p id="notice"><%= notice %></p>

<h1>Carmodels</h1>

<table>
  <thead>
    <tr>
      <th>Type1</th>
      <th>Fuel1</th>
      <th>Range1</th>
      <th>Speed1</th>
      <th>Pollution1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @carmodels.each do |carmodel| %>
      <tr>
        <td><%= carmodel.type1 %></td>
        <td><%= carmodel.fuel1 %></td>
        <td><%= carmodel.range1 %></td>
        <td><%= carmodel.speed1 %></td>
        <td><%= carmodel.pollution1 %></td>
        <td><%= link_to 'Show', carmodel %></td>
        <td><%= link_to 'Edit', edit_carmodel_path(carmodel) %></td>
        <td><%= link_to 'Destroy', carmodel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Carmodel', new_carmodel_path %>
